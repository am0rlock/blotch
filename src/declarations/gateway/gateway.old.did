type Result = 
 variant {
   err: GatewayError;
   ok: principal;
 };
type GatewayError = 
 variant {
   PortalAlreadyExists;
   PortalNotFound;
 };
service : {
  createPortal: () -> (Result);
  getPortal: () -> (Result);
  isPortalPrincipalValid: (principal) -> (bool) query;
  subscribe: () -> ();
}
