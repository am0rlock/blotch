type Result = 
 variant {
   err: PostDatabaseError;
   ok: vec PostID;
 };
type PostID = 
 record {
   id: nat64;
   portalPrincipal: principal;
 };
type PostDatabaseError = variant {InvalidRange;};
service : {
  getTopPosts: (nat64, int64) -> (Result) query;
  initialize: () -> ();
  notifyNewPortal: (principal) -> ();
  notifyPostUpdate: (PostID) -> ();
}
